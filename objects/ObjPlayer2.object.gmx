    }   
}
else if(!Jump and 
(!Up and !Down))
{ 
    sprite_index = Animations[0];
}

if x&gt;(view_xview[0]+32) 
{
x += -Left * 7;
}

if x&lt;(view_xview[0]+608)
{
x += Right*7;
}

// y 
