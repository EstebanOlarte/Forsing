{
    Up = keyboard_check(ord("I"));
    Down = keyboard_check(ord("K"));
    Left = keyboard_check(ord("J"));
    Right = keyboard_check(ord("L"));
}
// x
if(Right - Left != 0)
{
    if(Right)
    {
        image_xscale = 1;
    }
    else if (Left)
    {
        image_xscale = -1;
    }
    if(!Jump) and (!OA)
    {
        sprite_index = Animations[1];
    }   
}
else if(!Jump and 
(!Up and !Down))
{ 
    if(!OA)
    {
        sprite_index = Animations[0];
    }
}
if x &gt; (view_xview[0]+32) 
{
    x += -Left * 7;
}

if x &lt; (view_xview[0]+608)
{
    x += Right*7;
}

// y 
if(Up and Z &gt; 0 and !Down )
{ 
    MoveSpeed = 5;
    if(Z - 5 &lt; 0)
    {
        MoveSpeed = 1;
    }
    if(Jump)
    {
        Z -= MoveSpeed;
    }
    y -= MoveSpeed;
    if(!Jump and Z &gt; 0) and (!OA)
    {
        sprite_index = Animations[1]
    }
}
if(Down and Z &lt; 128 and !Up)
{    MoveSpeed = 5;
    if(Z + 5 &gt; 128)
