{
    Space = keyboard_check_pressed(vk_alt);
}
if(vspeed == MaxJumpForce)
{
    vspeed = 0;
    Jump = false;
    Y --;
    sprite_index = Animations[0];   
}

if(Jump)
{
    if(y + sprite_height/2 &lt; LastY)
    {
    // Up
        Y ++;
    }
    else if(y + sprite_height/2 &gt; LastY)
    {
    // Down
        Y --;      
    }   
    vspeed += Gravity;
    LastY = y + sprite_height/2; 
}
if(Space and !Jump)
{
    vspeed -= MaxJumpForce;
    Jump = true; 
    sprite_index = Animations[2];
}
